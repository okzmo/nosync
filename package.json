{
  "name": "dumpi",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev:web",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,svelte}\"",
    "db:up": "docker compose -f ./compose.db.yaml up -d",
    "db:down": "docker compose -f ./compose.db.yaml down"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
